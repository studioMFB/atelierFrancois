    "primevue": "^3.48.1",
    "sass": "^1.83.0",
    "sortablejs": "^1.15.0",
    "three": "^0.156.1",
    "three-addons": "^1.2.0",
    "three-subdivide": "^1.1.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-object-inspector": "^1.0.12",
    "vue-progressive-image": "^4.0.0",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.19"
