{
  "include": ["dist", "./src/components/modelViewer/textures/terrains.json"],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "node",
      "vite/client",
    ],
    "jsx": "react",
    "jsxFactory": "jsx",
    "baseUrl": "dist",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES6",
    "allowJs": false,
    "noImplicitAny": false,
    "strict": false,
    "noEmit": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
  }
}