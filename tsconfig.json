{
  "include": ["dist/assets/", "src/main.ts", "env.d.ts", "src/**/*", "src/**/*.ts", "src/**/*.tsx", "src/**/*.d.ts", "src/**/*.vue"],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "node",
      "vite/client",
    ],
    "jsx": "react",
    "jsxFactory": "jsx",
    "baseUrl": "./src",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "module": "ESNext",
    "target": "es5",
    "allowJs": false,
    "noImplicitAny": false,
    "strict": false,
    "noEmit": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
  }
}